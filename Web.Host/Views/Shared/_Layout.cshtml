<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @{ Html.RenderPartial("../Control/CommonControl");}
    @RenderSection("Head", required: false)
</head>
<body class="no-skin">
    @{ Html.RenderPartial("../Control/NavigationControl");}
    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try {
                ace.settings.check('main-container', 'fixed');
            } catch (e) { }
        </script>
        @{ Html.RenderAction("SysMenuControl", "Control");}
        <div class="main-content">
            <div class="main-content-inner">
                @{ Html.RenderPartial("../Control/HeadNavigationCtl");}
                <div class="page-content" style="padding:2px 10px 5px;">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
