@model List<Portal.Dto.ApiPermission>
@if (Model.Count() == 0)
{
     <div>没有API权限</div>
}
else
{ 
foreach (var apiPe in Model)
{//API
    <div>
        <label title="@apiPe.Name">
            <input appid="@apiPe.ApplicationId" type="checkbox" value="@apiPe.Code" /><span class="lbl">@apiPe.Name</span>
        </label>
    </div>
}
}
